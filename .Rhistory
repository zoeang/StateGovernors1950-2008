pv<-'http://www.presidency.ucsb.edu/showelection.php?year=1988'
pvt<-readHTMLTable(pv, header=T, stringAsFactors=F)
pvtab1<-as.data.frame(pvt[12])
View(pvtab1)
pvtab2<-pvtab1[c(10:60),c(1,2,3,4, 6,7)]
pvtab2[,c(7,8)]<-NA
pvtab2[,9]<-1988
